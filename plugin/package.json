{
  "name": "strapi-plugin-audit-viewer",
  "version": "1.0.0",
  "description": "Audit log viewer for Strapi v5 Admin Panel - Read-only access to PostgreSQL partition tables (Community Edition)",
  "license": "MIT",
  "author": "Mutlu Gercek",
  "homepage": "https://github.com/mutlugercek/strapi-plugin-audit-viewer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutlugercek/strapi-plugin-audit-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/mutlugercek/strapi-plugin-audit-viewer/issues"
  },
  "strapi": {
    "displayName": "Audit Log Viewer",
    "name": "audit-viewer",
    "description": "View audit logs from PostgreSQL partition tables",
    "kind": "plugin"
  },
  "type": "commonjs",
  "main": "./dist/server/index.js",
  "module": "./dist/server/index.mjs",
  "exports": {
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.tsx",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "strapi-admin.js",
    "strapi-server.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.2.6",
    "@strapi/strapi": "^5.0.0",
    "@strapi/types": "^5.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@strapi/design-system": "^2.0.0-rc.0",
    "@strapi/icons": "^2.0.0-rc.0",
    "@strapi/strapi": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "keywords": [
    "strapi",
    "strapi-plugin",
    "strapi-v5",
    "audit",
    "audit-log",
    "logging",
    "postgresql",
    "admin-panel",
    "security",
    "compliance"
  ],
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  }
}
